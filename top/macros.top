!2[$block] {
    $block $block
}

!3[$block] {
    !2[{$block}] $block
}

!4[$block] {
    !2[$block] !2[$block]
}

!5[$block] {
    !4[$block] $block
}

!6[$block] {
    !3[$block] !3[$block]
}

!7[$block] {
    !4[$block] !3[$block]
}

!8[$block] {
    !4[$block] !4[$block]
}

!9[$block] {
    !5[$block] !4[$block]
}

!apply3[$block] {
    !3[{$block rot}]
}

!for[$n $e $block] {
    $n while dup $e < {
        $block
        1 +
    }
}
